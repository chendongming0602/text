.other{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100vh;
  z-index: 999;
  background: rgba(1,1,1,0.7);
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 50rpx;
  overflow: hidden;
}
.content{
  position: relative;
}
.redBox{
  width: 616rpx;
  height: 587rpx;
}
.redBoxTwo{
  width: 645rpx;
  height: 644rpx;
  position: absolute;
  top: -5%;
  left: 50%;
  margin-left: -322.5rpx
}
.bot{
  width: 286rpx;
  height: 82rpx;
  margin: 34rpx auto;
}
.closeRed{
  width: 62rpx;
  height: 62rpx;
  margin: 48rpx auto;
}
@keyframes redBoxTwoA{
  0%{
    transform: rotate(0deg);
  }
  10%{
    transform: rotate(10deg)
  }
  20%{
    transform: rotate(-15deg);
  }
  30%{
    transform: rotate(0deg)
  }
  40%{
    transform: rotate(-5deg);
  }
  80%{
    transform: rotate(5deg)
  }
  100%{
    transform: rotate(0deg)
  }
}
.redBoxTwoA{
  animation: redBoxTwoA 1s linear infinite;
}